<div class="d-flex justify-content-center">
  <h1>ユーザー一覧</h1>
</div>
<div class="container">
  <div class="row">
  <% @users.each do |user| %>
    <% unless user.guest_user? %>
      <%= link_to user_path(user.id) do %>
        <%= image_tag user.get_profile_image(100, 100), class: "mt-3 rounded-circle"; %>
        <h6><%= user.name %></h6>
      <% end %>
    <% end %>
  <% end %>
  <%= paginate @users %>
  </div>
</div>