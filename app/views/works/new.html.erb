<h1>作品を投稿</h1>
<%= form_with model: @work do |f| %>
  <h4>楽曲</h4>
  <%= f.file_field :music_file, accept: "audio/*" %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>説明文</h4>
  <%= f.text_area :introduction %>
  <h4>タグ</h4>
  <%= f.text_field :tag_name, id:'tag_ids', :size=>"50", placeholder: "タグをつける。複数つけるには' , 'で区切ってください。" %> <br />
  <%= f.submit '投稿' %>
  <% end %>