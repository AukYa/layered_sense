<div class="d-flex justify-content-center">
  <h1 class="page_title">All Users</h1>
</div>
<div class="container">
  <div class="row justify-content-center">
    <div class="col-auto">
      <% @users.each do |user| %>
      　<% unless user.guest_user? %>
        　●
          <%=  link_to admin_user_path(user) do %>
            <strong class="text-dark"><%= user.name %></strong>
          <% end %>
          - <%= user.email %>
        <% end %>
      <% end %>
    </div>
  </div>
</div>